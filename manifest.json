{
  "manifest_version": 2,
  "name": "Form AutoFiller",
  "version": "1.0",
  "description": "autofill form fields with predefined values",
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": ["fillerBackground.js"],
    "persistent": false
  },
  "commands": {
    "fill-form": {
      "suggested_key": {
        "default": "Ctrl+Shift+F"
      },
      "description": "Fill form"
    }
  },
  "browser_specific_settings": {
    "id": "fillform@ndvo.blog.br"
  }
}
